{
    "name": "DoumanAsh.github.io",
    "version": "1.0.0",
    "private": true,
    "description": "Static site generator",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint . --ext .js --ignore-pattern public",
        "gen_data": "node ci/get_images.js",
        "start": "webpack-dev-server",
        "build": "webpack",
        "build:p": "webpack -p",
        "build:profile": "webpack --profile --json > stats.json",
        "build:analyze": "webpack-bundle-analyzer stats.json",
        "build:s": "webpack-dev-server"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DoumanAsh/DoumanAsh.github.io.git"
    },
    "keywords": [],
    "author": "Douman <douman@gmx.se>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DoumanAsh/DoumanAsh.github.io/issues"
    },
    "homepage": "https://github.com/DoumanAsh/DoumanAsh.github.io#readme",
    "devDependencies": {
        "autoprefixer": "*",
        "babel-core": "*",
        "babel-loader": "*",
        "babel-plugin-inferno": "*",
        "babel-plugin-transform-object-rest-spread": "*",
        "babel-preset-es2015": "*",
        "css-loader": "*",
        "dateformat": "*",
        "eslint": "*",
        "eslint-loader": "*",
        "eslint-plugin-compat": "*",
        "extract-text-webpack-plugin": "*",
        "file-loader": "*",
        "html-webpack-plugin": "*",
        "json5-loader": "*",
        "jstransformer-markdown-it": "*",
        "postcss-import": "*",
        "postcss-loader": "*",
        "postcss-nesting": "*",
        "pug": "^2.0.0-rc.1",
        "pug-loader": "*",
        "raw-loader": "*",
        "script-ext-html-webpack-plugin": "*",
        "stylelint": "*",
        "stylelint-config-standard": "*",
        "sugarss": "*",
        "moment": "*",
        "webpack": "*",
        "webpack-dev-server": "*"
    },
    "babel": {
        "presets": [
            [
                "es2015",
                {
                    "modules": false
                }
            ]
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    },
    "browserslist": [
        "Last 2 versions",
        "ie 11"
    ]
}
